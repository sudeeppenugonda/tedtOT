				$scope.clientSelected = function(client){
					console.log(client);
					if(client.match(/\bTINE/gi)){
						console.log("TINE");
						document.getElementById("datepicker").style.display="block";
						document.getElementById("geolocation").style.display="none";
						$scope.overList=['** Produkt mangler **','TINE Overordnet','Drikke','Frokost og lunsj','Mellommåltid','Turtelkuene','Middag, matlaging, dessert','SØK','Sunniva','Matfløte'];
						$scope.productList=['** Produkt mangler **','Felleskampanje','Julekampanje','Kommunikasjon','Profil','SØK','TINE Partner','TINEStafetten','Biola','Barer','Go\'Morgen','Jarlsberg','TINE Litago','Uten','Turtelkuene','Lettere Felleskampanje','Norvegia','Norvegia Laktosefri','TINE Norvegia Lettere','NÅ','Osteriet','Piano','TINE Piano Vaniljekrem','Vaniljesaus','Vaniljesaus Zero','Revet Ost','TINE Rislunsj','TINE Sprett','TINE Sprett Drikkeyoghurt',
											'IsTe','Juice','Noisy','Sunniva felles','Sunniva Lett','Sunniva mormerke','Sunniva Original','Sunniva Premium','Sunniva Presset','SUPRI','TINE 14','TINE Brunost','TINE Brunost Junior','TINE Hvit Geitost','Dulce','Julelatte','TINE Iskaffe','Zero','TINE Kremgo','Matfløte','Cottage Cheese','Creme Fraiche','Felles','Kesam','Lettrømme','Matsans','Rømme','Smør','TINE Kremfløte','Melk','Styrk','TINE Yoghurt','TINE Yoghurt Fyldig','YT','TINE Østavind'];
					}
					else if(client.match(/\bSpareBank 1/gi)){
						console.log("SpareBank 1");
						document.getElementById("datepicker").style.display="none";
						document.getElementById("geolocation").style.display="block";
						$scope.overList=['Andel Forsikring','Byttehelg','Nasjonal ASK','Pensjonskonto','Spleis','SØK','** Produkt mangler **','ASK','BFFT','Boliglån','Personforsikring','BM','IPS','Jobbannonser','SPINN','mCASH','Aksjesparekonto','TUT','PETRO Finans','Vipps','Kundeutbytte','Merkevare'];
						$scope.productList=['Andel Forsikring','Byttehelg','Nasjonal ASK','Pensjonskonto','Spleis','SØK','** Produkt mangler **','ASK','BFFT','Boliglån','Personforsikring','BM','IPS','Jobbannonser','SPINN','mCASH','Aksjesparekonto','TUT','PETRO Finans','Vipps','Kundeutbytte','Merkevare'];
					}
					else{
						document.getElementById("datepicker").style.display="none";
						document.getElementById("geolocation").style.display="none";
					}
					$scope.campaign.over="";
					$scope.campaign.product="";
					$scope.campaign.region="";
					$scope.date="";
					$scope.name="";
					$scope.info="";
					$scope.campaignName="";
					
				}
				
				$scope.overSelected = function(){
					$scope.campaign.product="";
					$scope.campaign.region="";
					$scope.date="";
					$scope.name="";
					$scope.info="";
					$scope.campaignName="";
				}
				
				$scope.productSelected = function(){
					$scope.campaign.region="";
					$scope.date="";
					$scope.name="";
					$scope.info="";
					$scope.campaignName="";
				}
				
				$scope.dateSelected = function(){
					$scope.campaign.region="";
					$scope.name="";
					$scope.info="";
					$scope.campaignName="";
				}
				
				$scope.regionSelected = function(){
					$scope.date="";
					$scope.name="";
					$scope.info="";
					$scope.campaignName="";
				}
				
				$scope.getCampaignNAme = function(){
					console.log($scope.name);
					if($scope.campaign.client.match(/\bTINE/gi)){
						$scope.campaignName=$scope.date.split(',')[1] + '_' + $scope.date.split(' ')[1].substring(0, 3) + '_' + $scope.campaign.over + '_' + $scope.campaign.product + '_' + $scope.name;
					}
					else if($scope.campaign.client.match(/\bSpareBank 1/gi)){
						$scope.campaignName=$scope.campaign.region + '_' + $scope.campaign.over + '_' + $scope.campaign.product + '_' + $scope.name;
					}
					else
						$scope.campaignName=$scope.campaign.client + '_' + $scope.campaign.over + '_' + $scope.campaign.product + '_' + $scope.name;
				}
				
				$('.datepicker').pickadate({
					selectMonths: true, // Creates a dropdown to control month
					selectYears: 15, // Creates a dropdown of 15 years to control year,
					today: 'Today',
					clear: 'Clear',
					close: 'Ok',
					closeOnSelect: true // Close upon selecting a date,
				  });
				
				$scope.copy = function(){
				   if($scope.campaignName!=""){
					var copyText = document.getElementById("campaignName");
					copyText.select();
					document.execCommand("Copy");
					//alert("Copied the text: " + copyText.value);
					var $toastContent = $('<span>Kopierte teksten</span>');
					Materialize.toast($toastContent, 5000, 'rounded');
				   }
				   else{
					var $toastContent = $('<span>vennligst fyll ut alle detaljer</span>');
					Materialize.toast($toastContent, 5000, 'rounded');
					}
				}
				
				//save.....
				
			});
			
